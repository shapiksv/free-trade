{
  "version": 3,
  "name": "Example Gateway",
  "port": 9191,
  "echo_endpoint": true,
  "output_encoding": "no-op",
  "timeout": "2000ms",
  "endpoints": [
    {
      "endpoint": "/api/v1/users",
      "method": "GET",
      "timeout": "10s",
      "input_headers": [
        "Content-Type",
        "Authorization"
      ],
      "backend": [
        {
          "host": [
            "http://host.docker.internal:8081"
          ],
          "url_pattern": "/api/v1/users"
        }
      ]
    }
  ]
}